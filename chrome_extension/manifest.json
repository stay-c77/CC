{
  "manifest_version": 3,
  "name": "CC",
  "description": "A productivity extension with sound and timer functionality.",
  "version": "1.0",
  "permissions":[
    "webNavigation",
    "webRequest",
    "storage",
    "tabs",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.jpg",
    "48": "icons/icon48.jpg",
    "128": "icons/icon128.jpg"
  },
  "web_accessible_resources": [
  {
    "resources": ["sounds/*.mp3"],
    "matches": ["<all_urls>"]
  }
]

}
